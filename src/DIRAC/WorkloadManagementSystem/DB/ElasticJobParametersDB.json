{
  "index_patterns": ["job_parameter_*"],
  "template": {
    "settings": {
      "index": {
        "number_of_shards": 1,
        "number_of_replicas": 1          
      }
    },
    "mappings": {
      "properties": {
        "JobID": {
          "type": "long"
        },
        "Status": {
          "type": "keyword"
        },
        "timestamp": {
          "type": "date"
        },
        "PilotAgent": {
          "type": "keyword"
        },
        "Pilot_Reference": {
          "type": "keyword"
        },
        "JobGroup": {
          "type": "keyword"
        },
        "JobType": {
          "type": "keyword"
        },
        "JobWrapperPID": {
          "type": "long"
        },
        "CPUNormalizationFactor": {
          "type": "long"
        },
        "NormCPUTime(s)": {
          "type": "long"
        },
        "Memory(MB)": {
          "type": "long"
        },
        "ModelName": {
          "type": "keyword"
        },
        "LocalAccount": {
          "type": "keyword"
        },
        "HostName": {
          "type": "text"
        },
        "TotalCPUTime(s)": {
          "type": "long"
        },
        "PayloadPID": {
          "type": "long"
        },
        "CEQueue": {
          "type": "keyword"
        },
        "BatchSystem": {
          "type": "keyword"
        },
        "GridCE": {
          "type": "keyword"
        }
      }
    }  
  }
}
